{
	"name": "talla-ui",
	"version": "4.0.0-dev.103",
	"publishConfig": {
		"tag": "next",
		"access": "public"
	},
	"description": "TÃ¤lla UI framework",
	"author": "Jelmer Cormont",
	"license": "MIT",
	"homepage": "https://talla-ui.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/talla-ui/talla-ui"
	},
	"type": "module",
	"main": "index.js",
	"module": "index.js",
	"types": "index.d.ts",
	"files": [
		"index.js",
		"index.d.ts"
	],
	"scripts": {
		"setup": "npm install && concurrently npm:setup:*",
		"setup:util": "cd packages/util && npm install",
		"setup:core": "cd packages/core && npm install",
		"setup:test": "cd packages/test-handler && npm install",
		"setup:web": "cd packages/web-handler && npm install",
		"setup:docgen": "cd packages/docgen && npm install",
		"setup:docs": "cd docs && npm install",
		"build": "npm run setup && npm run build:util && npm run build:core && npm run build:test && npm run build:web",
		"build:util": "cd packages/util && npm run build",
		"build:core": "cd packages/core && npm run build",
		"build:test": "cd packages/test-handler && npm run build",
		"build:web": "cd packages/web-handler && npm run build",
		"build:docgen": "cd packages/docgen && npm run build",
		"postbuild": "npm run check-docs",
		"clean": "concurrently npm:clean:*",
		"clean:util": "cd packages/util && npm run clean",
		"clean:core": "cd packages/core && npm run clean",
		"clean:test": "cd packages/test-handler && npm run clean",
		"clean:web": "cd packages/web-handler && npm run clean",
		"clean:docgen": "cd packages/docgen && npm run clean",
		"clean:docs": "cd docs && npm run clean",
		"test": "vitest run --silent=true",
		"test:coverage": "vitest --coverage",
		"test:util": "cd packages/util && npm test",
		"test:core": "cd packages/core && npm test",
		"test:test": "cd packages/test-handler && npm test",
		"buildtest:web-esbuild": "cd buildtest/web-esbuild && npm install && npm start",
		"buildtest:web-esm-tsc": "cd buildtest/web-esm-tsc && npm install && npm start",
		"buildtest:web-esm-js": "cd buildtest/web-esm-js && npm install && npm start",
		"buildtest:web-iife": "cd buildtest/web-iife && npm install && npm start",
		"buildtest:web-parcel": "cd buildtest/web-parcel && npm install && npm start",
		"buildtest:web-webpack": "cd buildtest/web-webpack && npm install && npm start",
		"buildtest:web-vite": "cd buildtest/web-vite && npm install && npm start",
		"buildtest:web-vite:test": "cd buildtest/web-vite && npm install && npm test",
		"check-format": " concurrently npm:check-format:* && echo OK",
		"check-format:other": "prettier --check \"*.md\" \"**/{package,tsconfig,config,launch,tasks}.json\"",
		"check-format:util": "cd packages/util && npm run check-format",
		"check-format:core": "cd packages/core && npm run check-format",
		"check-format:test": "cd packages/test-handler && npm run check-format",
		"check-format:web": "cd packages/web-handler && npm run check-format",
		"check-format:docgen": "cd packages/docgen && npm run check-format",
		"check-format:docs": "cd docs && npm run check-format",
		"check-docs": "npm run build:docgen && npm run www-build:check-docs",
		"www-build": "npm run build:docgen && npm run www-build:docs",
		"www-build:docs": "cd docs && npm run build",
		"www-build:check-docs": "cd docs && npm run check-docs",
		"www-serve": "cd docs && npm run serve",
		"www-build-site-only": "cd docs && npm run build:site",
		"version": "node scripts/set-version.js && npm run build && git add .",
		"publish-all": "concurrently npm:publish:util && concurrently npm:publish:core && concurrently npm:publish:packages:* && npm publish --silent",
		"publish:util": "cd packages/util && npm publish --silent",
		"publish:core": "cd packages/core && npm publish --silent",
		"publish:packages:test-handler": "cd packages/test-handler && npm publish --silent",
		"publish:packages:web-handler": "cd packages/web-handler && npm publish --silent",
		"publish:packages:docgen": "cd packages/docgen && npm publish --silent"
	},
	"prettier": {
		"useTabs": true
	},
	"dependencies": {
		"@talla-ui/core": "4.0.0-dev.103",
		"@talla-ui/util": "4.0.0-dev.103"
	},
	"devDependencies": {
		"@talla-ui/core": "file:packages/core",
		"@talla-ui/util": "file:packages/util",
		"@types/node": "20",
		"@vitest/coverage-v8": "3",
		"concurrently": "latest",
		"esbuild": "^0.23.1",
		"http-server": "latest",
		"prettier": "^3.3.3",
		"rimraf": "latest",
		"tslib": "^2.7.0",
		"typescript": "5.6",
		"vitest": "3"
	}
}
