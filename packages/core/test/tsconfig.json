{
	"compilerOptions": {
		"noEmit": true,
		"stripInternal": true,
		"useDefineForClassFields": true,
		"strict": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitOverride": true,
		"noUncheckedIndexedAccess": true,
		"module": "esnext",
		"moduleResolution": "node",
		"target": "ES2022",
		"lib": ["dom", "esnext"],
		"skipLibCheck": true
	}
}
