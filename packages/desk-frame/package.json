{
	"private": true,
	"name": "desk-frame",
	"version": "0.0.0",
	"description": "",
	"keywords": [],
	"author": "",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"clean": "rimraf coverage dist dist-* .test-run",
		"prebuild": "npm run clean",
		"build": "run-p build:*",
		"build:esnext": "tsc -p src --declaration",
		"build:es2020": "tsc -p src --target es2020 --outdir dist-es2020",
		"build:es2018": "tsc -p src --target es2018 --outdir dist-es2018",
		"build:es2015": "tsc -p src --target es2015 --outdir dist-es2015",
		"dts": "tsc -p src --declaration --emitDeclarationOnly",
		"pretest": "tsc -p test",
		"test": "node .test-run",
		"posttest": "rimraf .test-run",
		"pretest-c8": "tsc -p test",
		"test-c8": "c8 node .test-run",
		"posttest-c8": "rimraf .test-run"
	},
	"prettier": {
		"useTabs": true
	},
	"c8": {
		"reporter": [
			"html"
		],
		"exclude": [
			".test-run"
		]
	},
	"files": [
		"dist",
		"dist-es2015",
		"dist-es2018",
		"dist-es2020"
	],
	"dependencies": {
		"tslib": "^2.5.0"
	},
	"devDependencies": {
		"@desk-framework/test": "file:../test"
	}
}
